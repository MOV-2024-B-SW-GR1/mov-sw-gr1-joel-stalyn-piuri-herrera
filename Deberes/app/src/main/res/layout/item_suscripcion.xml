<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- TextView para mostrar la fecha de inicio de la suscripción -->
    <TextView
        android:id="@+id/textViewFechaInicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha de Inicio"
        android:padding="16dp"/>

    <!-- TextView para mostrar el método de pago de la suscripción -->
    <TextView
        android:id="@+id/textViewMetodoPago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Método de Pago"
        android:padding="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewFechaInicio"/>
</androidx.constraintlayout.widget.ConstraintLayout>
